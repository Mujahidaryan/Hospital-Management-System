<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Admin Dashboard</title>
  <link rel="stylesheet" href="/styles.css"> <!-- optional -->
</head>
<body>
  <h1>Admin Dashboard</h1>

  <section>
    <h2>Doctors List</h2>
    <ul>
      <% doctors.forEach(doctor => { %>
        <li><%= doctor.name %> — <%= doctor.specialty %></li>
      <% }); %>
    </ul>
  </section>

  <section>
    <h2>Patients List</h2>
    <ul>
      <% patients.forEach(patient => { %>
        <li><%= patient.name %> — <%= patient.disease %></li>
      <% }); %>
    </ul>
  </section>
</body>
</html>
